	month = apr,
	year = {2018},
	note = {Publisher: Public Library of Science},
	keywords = {Genetic loci, Phylogenetics, Genomics, Antigenic variation, Conjugate vaccines, Population genetics, Species diversity, Vaccines},
	pages = {e1006966},
